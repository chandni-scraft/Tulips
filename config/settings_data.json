      " /* testimonial home page */ /* Impact Line Container */.impact-line {display: inline-flex; align-items: center; gap: 10px; font-weight: bold; font-size: 20px; color: #ff6b35; padding: 15px 25px; background: linear-gradient(45deg, #fff5f0, #ffe8d6); border-radius: 25px; box-shadow: 0 4px 15px rgba(255, 107, 53, 0.2); animation: slideInFromTop 1.5s ease-out; margin: 20px 0;}",
      " /* Star Animation */.impact-line .star {animation: bounce 2s infinite; font-size: 24px;}",
      " /* Slide In Animation */@keyframes slideInFromTop {0% {opacity: 0; transform: translateY(-30px) scale(0.8); } 50% {opacity: 0.7; transform: translateY(-10px) scale(0.9); } 100% {opacity: 1; transform: translateY(0) scale(1); }}",
      " /* Bounce Animation for Star */@keyframes bounce {0%, 20%, 50%, 80%, 100% {transform: translateY(0); } 40% {transform: translateY(-8px); } 60% {transform: translateY(-4px); }}",
      " /* Hover Effect */.impact-line:hover {transform: translateY(-2px); box-shadow: 0 6px 20px rgba(255, 107, 53, 0.3); transition: all 0.3s ease;}"
